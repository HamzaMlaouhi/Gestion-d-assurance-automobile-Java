<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="305.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Administration.SceneBuilder.ListeCompagnieController">
   <children>
      <TableView fx:id="table" layoutX="6.0" layoutY="53.0" onKeyPressed="#search" prefHeight="200.0" prefWidth="589.0">
        <columns>
            <TableColumn fx:id="id" prefWidth="93.0" text="ID" />
          <TableColumn fx:id="nomC" prefWidth="127.0" text="Nom Compagnie" />
          <TableColumn fx:id="adr" prefWidth="101.0" text="Adresse" />
            <TableColumn fx:id="fix" prefWidth="73.0" text="Tel" />
            <TableColumn fx:id="fax" prefWidth="99.0" text="Faxe" />
            <TableColumn fx:id="site" prefWidth="94.0" text="Site" />
        </columns>
      </TableView>
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" style="-fx-background-color: black;" text="Retour" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <TextField fx:id="rechercher" layoutX="430.0" layoutY="14.0" onKeyPressed="#search" promptText="Filtrer par adresse" />
   </children>
</AnchorPane>
