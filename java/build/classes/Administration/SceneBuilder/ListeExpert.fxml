<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="328.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Administration.SceneBuilder.ListeExpertController">
    <children>
      <TableView fx:id="table" layoutX="6.0" layoutY="53.0" prefHeight="200.0" prefWidth="589.0">
        <columns>
            <TableColumn fx:id="cin" prefWidth="108.0" text="CIN" />
          <TableColumn fx:id="nom" prefWidth="123.0" text="Nom" />
          <TableColumn fx:id="prenom" minWidth="2.0" prefWidth="117.0" text="Prenom" />
            <TableColumn fx:id="tel" prefWidth="111.0" text="Tel" />
            <TableColumn fx:id="zone" prefWidth="128.0" text="Zone" />
        </columns>
      </TableView>
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" style="-fx-background-color: black;" text="Retour" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="disabled" layoutX="452.0" layoutY="264.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="143.0" style="-fx-background-color: lightgreen;" text="Liste Expert en attente">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <TextField fx:id="rechercher" layoutX="422.0" layoutY="14.0" onKeyPressed="#search" promptText="Filtrer par Zone" />
   </children>
    
</AnchorPane>
